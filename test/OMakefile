CXXFLAGS += -Wall -O0 -g -I../include
LDFLAGS += -levent -lcrypto
LIBS += ../src/libcage


.PHONY: clean rdp_test nodes_10000

rdp_test: $(CXXProgram rdp_test, rdp_test)
nodes_10000: $(CXXProgram nodes_10000, nodes_10000)

clean:
	rm -f *~ *.o
	rm -f rdp_test nodes_10000

.DEFAULT: rdp_test nodes_10000
