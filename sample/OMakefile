CXXFLAGS += -Wall -O2 -g -I../include
LDFLAGS += -levent -lcrypto
LIBS += ../src/libcage


.PHONY: clean sample1 sample2 sample3 sample4

sample1: $(CXXProgram sample1, sample1)
sample2: $(CXXProgram sample2, sample2)
sample2: $(CXXProgram sample3, sample3)
sample4: $(CXXProgram sample4, sample4)

clean:
	rm -f *~ *.o
	rm -f sample1 sample2 sample3 sample4

.DEFAULT: sample1 sample2 sample3 sample4
